msgstr "SSH Kapcsolatok"

#. settings-schema.json->page-terminal-settings->title
#. settings-schema.json->section-terminal-app->title
msgid "Terminal Application"
msgstr "Terminál alkalmazás"

#. settings-schema.json->section-connections-list->title
msgid "SSH Connections List"
msgstr "SSH kapcsolatok listája"

#. settings-schema.json->section-setup-title-flag->title
