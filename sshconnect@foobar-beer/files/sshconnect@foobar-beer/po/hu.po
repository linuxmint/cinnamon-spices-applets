msgstr "Terminál csempe"

#. settings-schema.json->section-setup-profile-flag->title
#. settings-schema.json->connections->columns->title
msgid "Terminal Profile"
msgstr "Terminál profil"

#. settings-schema.json->connections->description
msgid "Connections List"
msgstr "Kapcsolatok listája"

#. settings-schema.json->connections->columns->title
msgid "Connection Name"
msgstr "Kapcsolat neve"

#. settings-schema.json->connections->columns->title
msgid "Group Name"
msgstr "Csoport neve"

