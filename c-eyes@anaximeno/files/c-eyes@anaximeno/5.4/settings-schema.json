{
	"layout": {
		"type": "layout",
		"pages": [
			"eye",
			"click"
		],
		"eye": {
			"type": "page",
			"title": "Eye",
			"sections": [
				"eye-generic",
				"eye-colors"
			]
		},
		"click": {
			"type": "page",
			"title": "Click Animation",
			"sections": [
				"click-generic",
				"click-events",
				"click-colors"
			]
		},
		"eye-generic": {
			"type": "section",
			"title": "General Configuration",
			"keys": [
				"eye-mode",
				"eye-line-width",
				"eye-margin",
				"eye-repaint-interval"
			]
		},
		"eye-colors": {
			"type": "section",
			"title": "Eye Colors",
			"keys": [
				"eye-clicked-color",
				"iris-clicked-color",
				"pupil-clicked-color"
			]
		},
		"click-generic": {
			"type": "section",
			"title": "Generic",
			"keys": [
				"mouse-click-enable",
				"mouse-click-mode",
				"mouse-click-image-size"
			]
		},
		"click-events": {
			"type": "section",
			"title": "Events",
			"keys": [
				"mouse-left-click-enable",
				"mouse-middle-click-enable",
				"mouse-right-click-enable",
				"fade-timeout"
			],
			"dependency": "mouse-click-enable"
		},
		"click-colors": {
			"type": "section",
			"title": "Colors",
			"keys": [
				"mouse-left-click-color",
				"mouse-middle-click-color",
				"mouse-right-click-color",
				"mouse-click-opacity"
			],
			"dependency": "mouse-click-enable"
		}
	},
	"eye-mode": {
		"type": "combobox",
		"default": "lids",
		"description": "Eye Mode",
		"options": {
			"Lids": "lids",
			"Bulb": "bulb"
		}
	},
	"eye-line-width": {
		"type": "spinbutton",
		"description": "Eye Line Width",
		"default": 2,
		"min": 1,
		"max": 5,
		"step": 1,
		"units": "px"
	},
	"eye-margin": {
		"type": "spinbutton",
		"default": 4,
		"min": 1,
		"max": 10,
		"step": 1,
		"units": "px",
		"description": "Eye Margin Size"
	},
	"eye-repaint-interval": {
		"type": "spinbutton",
		"default": 60,
		"min": 5,
		"max": 200,
		"step": 5,
		"units": "ms",
		"description": "Eye Repaint Interval"
	},
	"eye-clicked-color": {
		"type": "colorchooser",
		"default": "#0c75de",
		"description": "Eye Clicked Color"
	},
	"iris-clicked-color": {
		"type": "colorchooser",
		"default": "#4077BF",
		"description": "Iris Clicked Color"
	},
	"pupil-clicked-color": {
		"type": "colorchooser",
		"default": "#40BEBF",
		"description": "Pupil Clicked Color"
	},
	"mouse-click-enable": {
		"type": "checkbox",
		"default": true,
		"description": "Mouse Click Enable"
	},
	"mouse-click-mode": {
		"type": "combobox",
		"default": 1,
		"description": "Mouse Click Mode",
		"options": {
			"Simple": 1,
			"Trigun": 2,
			"Nublo": 3,
			"Risco": 4,
			"Wolf": 5,
			"Pincel": 6,
			"Claw": 7,
			"Metropoli": 8,
			"Flowey": 9,
			"Kusco": 10,
			"Ouroboros": 11,
			"Helix": 12,
			"Direction": 13,
			"Spinks": 14,
			"Thatch": 15,
			"Cotton": 16,
			"Circle": 17,
			"Gear": 18
		},
		"dependency": "mouse-click-enable"
	},
	"mouse-click-image-size": {
		"type": "spinbutton",
		"default": 60,
		"min": 20,
		"max": 120,
		"step": 5,
		"units": "px",
		"description": "Mouse Click Size",
		"dependency": "mouse-click-enable"
	},
	"mouse-left-click-enable": {
		"type": "checkbox",
		"default": true,
		"description": "Enable Mouse Left Click"
	},
	"mouse-middle-click-enable": {
		"type": "checkbox",
		"default": true,
		"description": "Enable Mouse Middle Click"
	},
	"mouse-right-click-enable": {
		"type": "checkbox",
		"default": true,
		"description": "Enable Mouse Right Click"
	},
	"fade-timeout": {
		"type": "spinbutton",
		"default": 400,
		"min": 100,
		"max": 5000,
		"step": 100,
		"units": "ms",
		"description": "Click Fade Timeout"
	},
	"mouse-left-click-color": {
		"type": "colorchooser",
		"default": "#0c75de",
		"description": "Mouse Left Click Color",
		"dependency": "mouse-left-click-enable"
	},
	"mouse-middle-click-color": {
		"type": "colorchooser",
		"default": "#0c75de",
		"description": "Mouse Middle Click Color",
		"dependency": "mouse-middle-click-enable"
	},
	"mouse-right-click-color": {
		"type": "colorchooser",
		"default": "#0c75de",
		"description": "Mouse Right Click Color",
		"dependency": "mouse-right-click-enable"
	},
	"mouse-click-opacity": {
		"type": "spinbutton",
		"description": "Mouse Click Opacity",
		"default": 180,
		"min": 0,
		"max": 255,
		"step": 5
	}
}