{
    "head": {
        "type": "header",
        "description": "Settings for bose@tuuxx"
    },

    "address": {
        "type": "entry",
        "default": "XX:XX:XX:XX:XX:XX",
        "description": "Address of the Bluethooth BOSE device:",
        "tooltip": "Format: xx:xx:xx:xx:xx:xx"
    },

    "path": {
    "type": "entry",
    "default": ".local/share/cinnamon/applets/bose@tuuxx/based-connect/based-connect",
    "description": "Path of based-connect:",
    "tooltip": "Path to the based-connect tool"
    },

    "basedconnectLookup": {
    "type": "button",
    "indent": true,
    "description": "Get based-connect tool",
    "callback": "basedconnectLookup",
    "tooltip": "Opens git webpage of the based-connect tool"
    }
}
