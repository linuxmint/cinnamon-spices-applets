{
    "layout": {
        "type": "layout",
        "pages": [
            "window-settings",
            "workspace-settings",
            "application-settings"
        ],
        "window-settings": {
            "type": "page",
            "title": "Windows",
            "sections": ["window-section"]
        },
        "workspace-settings": {
            "type": "page",
            "title": "Workspace",
            "sections": ["workspace-section", "window-workspace-section"]
        },
        "application-settings": {
            "type": "page",
            "title": "Other",
            "sections": ["application-section"]
        },
        "window-section": {
            "type": "section",
            "title": "Window Arrangement - Configure the hotkeys to manage windows",
            "keys": [
                "arrangeKeyBinding",
                "focusKeyBinding",
                "moveKeyBinding",
                "swapKeyBinding",
                "halfScreenBinding",
                "fullScreenBinding",
                "unmaximizeBinding",
                "minimizeBinding",
                "killBinding"
            ]
        },
        "workspace-section": {
            "type": "section",
            "title": "Workspace arrangement\nConfigure the hotkeys to switch workspaces",
            "keys": [
                "switchWorkspace1Binding",
                "switchWorkspace2Binding",
                "switchWorkspace3Binding",
                "switchWorkspace4Binding",
                "switchWorkspace5Binding",
                "switchWorkspace6Binding",
                "switchWorkspace7Binding",
                "switchWorkspace8Binding",
                "switchWorkspace9Binding",
                "switchNextWorkspaceBinding",
                "switchPrevWorkspaceBinding"
            ]
        },
        "window-workspace-section": {
            "type": "section",
            "title": "Configure the hotkeys to move windows across workspaces",
            "keys": [
                "moveSwitchWorkspace1Binding",
                "moveSwitchWorkspace2Binding",
                "moveSwitchWorkspace3Binding",
                "moveSwitchWorkspace4Binding",
                "moveSwitchWorkspace5Binding",
                "moveSwitchWorkspace6Binding",
                "moveSwitchWorkspace7Binding",
                "moveSwitchWorkspace8Binding",
                "moveSwitchWorkspace9Binding",
                "moveSwitchNextWorkspaceBinding",
                "moveSwitchPrevWorkspaceBinding"
            ]
        },
        "application-section": {
            "type": "section",
            "title": "Other settings",
            "keys": [
                "shortcutConfigureNote",
                "custom-shortcuts-button",
                "toggleExtensionBinding"
            ]
        }
    },
    "arrangeKeyBinding": {
        "type": "keybinding",
        "description": "Arrange windows",
        "default": "<Super>Tab"
    },
    "focusKeyBinding": {
        "type": "keybinding",
        "description": "Focus window",
        "default": "<Super>f"
    },
    "moveKeyBinding": {
        "type": "keybinding",
        "description": "Move window",
        "default": "<Super>m"
    },
    "swapKeyBinding": {
        "type": "keybinding",
        "description": "Swap window",
        "default": "<Super>s"
    },
    "halfScreenBinding": {
        "type": "keybinding",
        "description": "Focused window occupy half screen",
        "default": "<Super>h"
    },
    "fullScreenBinding": {
        "type": "keybinding",
        "description": "Maximize the focused window",
        "default": "<Super>l"
    },
    "unmaximizeBinding": {
        "type": "keybinding",
        "description": "Exit from the maximized window",
        "default": "<Super>q"
    },
    "minimizeBinding": {
        "type": "keybinding",
        "description": "Minimize the focused window",
        "default": "<Super>p"
    },
    "moveSwitchWorkspace1Binding": {
        "type": "keybinding",
        "description": "Move Focused window to workspace 1",
        "default": "<Super><Shift>1"
    },
    "moveSwitchWorkspace2Binding": {
        "type": "keybinding",
        "description": "Move Focused window to workspace 2",
        "default": "<Super><Shift>2"
    },
    "moveSwitchWorkspace3Binding": {
        "type": "keybinding",
        "description": "Move Focused window to workspace 3",
        "default": "<Super><Shift>3"
    },
    "moveSwitchWorkspace4Binding": {
        "type": "keybinding",
        "description": "Move Focused window to workspace 4",
        "default": "<Super><Shift>4"
    },
    "moveSwitchWorkspace5Binding": {
        "type": "keybinding",
        "description": "Move Focused window to workspace 5",
        "default": "<Super><Shift>5"
    },
    "moveSwitchWorkspace6Binding": {
        "type": "keybinding",
        "description": "Move Focused window to workspace 6",
        "default": "<Super><Shift>6"
    },
    "moveSwitchWorkspace7Binding": {
        "type": "keybinding",
        "description": "Move Focused window to workspace 7",
        "default": "<Super><Shift>7"
    },
    "moveSwitchWorkspace8Binding": {
        "type": "keybinding",
        "description": "Move Focused window to workspace 8",
        "default": "<Super><Shift>8"
    },
    "moveSwitchWorkspace9Binding": {
        "type": "keybinding",
        "description": "Move Focused window to workspace 9",
        "default": "<Super><Shift>9"
    },
    "moveSwitchNextWorkspaceBinding": {
        "type": "keybinding",
        "description": "Move Focused window to next workspace",
        "default": "<Super><Shift>t"
    },
    "moveSwitchPrevWorkspaceBinding": {
        "type": "keybinding",
        "description": "Move Focused window to previous workspace",
        "default": "<Super><Shift>r"
    },
    "switchWorkspace1Binding": {
        "type": "keybinding",
        "description": "Switch to workspace 1",
        "default": "<Super>1"
    },
    "switchWorkspace2Binding": {
        "type": "keybinding",
        "description": "Switch to workspace 2",
        "default": "<Super>2"
    },
    "switchWorkspace3Binding": {
        "type": "keybinding",
        "description": "Switch to workspace 3",
        "default": "<Super>3"
    },
    "switchWorkspace4Binding": {
        "type": "keybinding",
        "description": "Switch to workspace 4",
        "default": "<Super>4"
    },
    "switchWorkspace5Binding": {
        "type": "keybinding",
        "description": "Switch to workspace 5",
        "default": "<Super>5"
    },
    "switchWorkspace6Binding": {
        "type": "keybinding",
        "description": "Switch to workspace 6",
        "default": "<Super>6"
    },
    "switchWorkspace7Binding": {
        "type": "keybinding",
        "description": "Switch to workspace 7",
        "default": "<Super>7"
    },
    "switchWorkspace8Binding": {
        "type": "keybinding",
        "description": "Switch to workspace 8",
        "default": "<Super>8"
    },
    "switchWorkspace9Binding": {
        "type": "keybinding",
        "description": "Switch to workspace 9",
        "default": "<Super>9"
    },
    "switchNextWorkspaceBinding": {
        "type": "keybinding",
        "description": "Switch to next workspace",
        "default": "<Super>t"
    },
    "switchPrevWorkspaceBinding": {
        "type": "keybinding",
        "description": "Switch to previous workspace",
        "default": "<Super>r"
    },
    "killBinding": {
        "type": "keybinding",
        "description": "Close the focused window",
        "default": "<Super>k"
    },
    "custom-shortcuts-button": {
        "type": "button",
        "description": "Open Keyboard Settings",
        "callback": "custom_shortcuts"
    },
    "shortcutConfigureNote": {
        "type": "label",
        "description": "Configure hotkeys for opening common applications\nYou can do this by going to: System Settings > Keyboard > Shortcuts > CustomShortcuts"
    },
    "toggleExtensionBinding": {
        "type": "keybinding",
        "description": "Enable/disable the extension",
        "default": "<Super>Escape"
    }
}