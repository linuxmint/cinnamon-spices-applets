{
	"wakatime-head": {
		"type": "header",
		"description": "WakaTime Settings"
	},
	"refreshInterval": {
		"type": "spinbutton",
		"default": 5,
		"min": 1,
		"max": 30,
		"step": 0.5,
		"units": "minutes",
		"description": "Refresh interval",
		"tooltip": "Increase or decrease this spinner value to change the refresh interval - use a slow refresh if you have a slow machine."
	},
	"apiKey": {
		"type": "entry",
		"default": "",
		"description": "WakaTime API Key",
		"tooltip": "The API key unique to your WakaTime account."
	},
	"getApiKey": {
		"type": "button",
		"description": "Open the WakaTime page to get your API key",
		"callback": "openApiKeyPage",
		"tooltip": "Opens WakaTime API key webpage"
	},
	"display-head": {
		"type": "header",
		"description": "Display Settings"
	},
	"logoColor": {
		"type": "radiogroup",
		"description": "WakaTime logo color",
		"default": "white",
		"options": {
			"White": "white",
			"Black": "black"
		}
	}
}