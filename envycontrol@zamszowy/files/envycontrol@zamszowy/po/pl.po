#. metadata.json->name
msgid "EnvyControl GUI"
msgstr "EnvyControl GUI"

#. metadata.json->description
msgid "Switches between between GPU modes on Nvidia Optimus systems."
msgstr ""
"Pozwala na przełączanie pomiędzy trybami GPU na systemach Nvidia Optimus."

#. settings-schema.json->section1->description
msgid "Nvidia Settings"
msgstr "Ustawienia trybu Nvidia"

#. settings-schema.json->enable-coolbits->description
msgid "Enable coolbits"
msgstr "Włącz coolbits"

#. settings-schema.json->enable-coolbits->tooltip
msgid ""
"Allows overclocking (not recommended)\n"
"See https://wiki.archlinux.org/title/NVIDIA/"
"Tips_and_tricks#Overclocking_and_cooling"
msgstr ""
"Pozwala na overclocking (nie zalecane)\n"
"Patrz: https://wiki.archlinux.org/title/NVIDIA/"
"Tips_and_tricks#Overclocking_and_cooling"

#. settings-schema.json->coolbits->description
msgid "Coolbits"
msgstr "Coolbits"

#. settings-schema.json->enable-forcecompositionpipeline->description
msgid "Enable ForceCompositionPipeline"
msgstr "Włącz ForceCompositionPipeline"

#. settings-schema.json->enable-forcecompositionpipeline->tooltip
msgid "Enable if facing screen tearing"
msgstr "Włącz jeśli występuje \"zrywanie obrazu\" (tearing)"

#. settings-schema.json->override-dm->description
msgid "Manually specify your Display Manager"
msgstr "Wybierz ręcznie Display Managera"

#. settings-schema.json->dm->description
msgid "Display manager"
msgstr "Display manager"

#. settings-schema.json->dm->options
msgid "gdm"
msgstr "gdm"

#. settings-schema.json->dm->options
msgid "gdm3"
msgstr "gdm3"

#. settings-schema.json->dm->options
msgid "sddm"
msgstr "sddm"

#. settings-schema.json->dm->options
msgid "lightdm"
msgstr "lightdm"

#. settings-schema.json->use-nvidia-current->description
msgid "Use nvidia-current instead of nvidia for kernel modules"
msgstr "Użyj modułu jądra nvidia-current zamiast nvidia"

#. settings-schema.json->use-nvidia-current->tooltip
msgid ""
"If nvidia kernel module is named nvidia-current on Debian, use this flag to "
"make EnvyControl use the correct module name"
msgstr ""
"Jeśli moduł jądra nvidii na Debianie nazywa się nvidia-current, użyj tej "
"opcji aby EnvyControl używał poprawnej nazwy modułu"

#. settings-schema.json->section2->description
msgid "Hybrid Settings"
msgstr "Ustawienia trybu hybrydowego"

#. settings-schema.json->enable-rtd3->description
msgid "Setup PCI-Express Runtime D3 (RTD3) Power Management"
msgstr "Konfiguracja zarządzania energią PCI-Express Runtime D3 (RTD3)"

#. settings-schema.json->enable-rtd3->tooltip
msgid "RTD3 allows the dGPU to be dynamically turned off when not in use"
msgstr "RTD3 umożliwia dynamiczne wyłączanie dGPU, gdy nie jest używane"

#. settings-schema.json->rtd3->description
msgid "RTD3"
msgstr "RTD3"

#. settings-schema.json->rtd3->options
msgid "0"
msgstr "0"

#. settings-schema.json->rtd3->options
msgid "1"
msgstr "1"

#. settings-schema.json->rtd3->options
msgid "2"
msgstr "2"

#. settings-schema.json->rtd3->options
msgid "3"
msgstr "3"

#. settings-schema.json->rtd3->tooltip
msgid ""
"0 - disabled\n"
"1 - coarse-grained\n"
"2 - fine-grained\n"
"3 - fine-grained for Ampere and later"
msgstr ""
"0 – wyłączone\n"
"1 – zgrubne (coarse-grained)\n"
"2 – szczegółowe (fine-grained)\n"
"3 – szczegółowe (fine-grained) dla Ampere i nowszych"
