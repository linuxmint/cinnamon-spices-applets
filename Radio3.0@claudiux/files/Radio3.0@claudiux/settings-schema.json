  "show-help-in-tabs": {
    "type": "switch",
    "description": "Show help labels in each tab of this Settings window",
    "default": true,
    "tooltip": "You can uncheck this option when you are familiar with all of these settings."
  },
  "stations-help": {
    "type": "textview",
    "default": "",
    "description": "Help",
    "height": 230,
    "dependency": "show-help-in-tabs"
  },
  "radios": {
    "type": "list",
    "description": "List of stations",
    "height": 396,
    "show-buttons": true,
    "reorderable": true,
    "columns": [
      {
        "id": "inc",
        "title": "Menu",
        "type": "boolean",
        "default": true,
        "align": 0.5
      },
      {
        "id": "play",
        "title": "♪/➟",
        "type": "boolean",
        "default": false,
        "align": 0.5
      },
      {
        "id": "name",
        "title": "Station or Category",
        "type": "string",
        "default": ""
      },
      {
        "id": "bitrate",
        "title": "Bitrate\n(kbps)",
        "type": "string",
        "default": "",
        "align": 0.5
      },
      {
        "id": "codec",
        "title": "Codec",
        "type": "string",
        "default": "",
        "align": 0.5
      },
      {
        "id": "url",
        "title": "Streaming URL (empty for a category)",
        "type": "string",
        "default": ""
      },
      {
        "id": "uuid",
        "title": "Unique identifier",
        "type": "string",
        "default": ""
      },
      {
        "id": "homepage",
        "title": "Home page",
        "type": "string",
        "default": ""
      },
      {
        "id": "tags",
        "title": "Tags",
        "type": "string",
        "default": ""
      },
      {
        "id": "favicon",
        "title": "Favicon",
        "type": "string",
        "default": ""
      }
    ],
    "default": [

    ]
  },
  "stations-list-play-button": {
    "type": "button",
    "description": "♪ Play the next station to test",
    "callback": "on_button_radios_play_clicked"
  },
  "category-to-move": {
    "type": "combobox",
    "description": "Category to which to move the selected stations:",
    "default": "",
    "options": {
      "(Undefined)": ""
    }
  },
  "button-moving": {
    "type": "button",
    "description": "➟ Move selected stations to this category",
    "callback": "on_button_radios_moving_clicked",
    "dependency": "category-to-move!="
  },
  "button_go_to_category": {
    "type": "button",
    "description": "Go to this category",
    "callback": "on_button_radios_go_to_category_clicked",
    "dependency": "category-to-move!="
  },
  "button-show-all": {
    "type": "button",
    "description": "Show all stations and categories in the menu",
    "callback": "on_button_radios_show_all_clicked"
  },
  "button-hide-all": {
    "type": "button",
    "description": "Hide all stations and categories (so the menu becomes empty)",
    "callback": "on_button_radios_hide_all_clicked"
  },
  "button-save": {
    "type": "button",
    "description": "Save this list of stations",
    "callback": "on_button_radios_save_clicked"
  },
  "button-restore": {
    "type": "button",
    "description": "Restore a list of stations",
