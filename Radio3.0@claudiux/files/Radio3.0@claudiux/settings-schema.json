  "button-update": {
    "type": "button",
    "description": "Update my station list with the Radio Database data",
    "callback": "on_button_radios_update_clicked"
  },
  "scale-update": {
    "type": "scale",
    "min": 0,
    "max": 100,
    "step": 1,
    "default": 0,
    "description": "%",
    "dependency": "show-scale-update"
  },
  "show-scale-update": {
    "type": "generic",
