{
    "layout" : {
        "type" : "layout",
        "pages" : ["menu", "panel", "sidebar", "quicklauncher"],

        "panel" : {
            "type" : "page",
            "title" : "Panel",
            "sections" : ["panel-appear", "panel-behave"]
        },
        "menu" : {
            "type" : "page",
            "title" : "Menu",
            "sections" : ["menu-appear", "menu-icons", "menu-behave"]
        },
        "sidebar" : {
            "type" : "page",
            "title" : "Sidebar",
            "sections" : ["sidebar-layout", "quitmenu-layout"]
        },
        "quicklauncher" : {
            "type" : "page",
            "title" : "Quicklauncher",
            "sections" : ["custom-quicklauncher"]
        },
        "panel-appear" : {
            "type" : "section",
            "title" : "Appearance",
            "keys" : ["menu-icon-custom", "menu-icon", "menu-label"]
        },
        "panel-behave" : {
            "type" : "section",
            "title" : "Behavior",
            "keys" : ["overlay-key", "activate-on-hover", "hover-delay", "enable-animation"]
        },
        "menu-appear" : {
            "type" : "section",
            "title" : "Layout and content",
            "keys" : ["menu-layout", "show-apps-description-on-buttons", "show-places", "menu-editor-button"]
        },
        "menu-icons" : {
            "type" : "section",
            "title" : "Icons and Labels",
            "keys" : ["show-category-icons", "show-application-icons", "show-context-menu-icons", "show-favorite-icons", "favorite-button-size", "all-programs-label", "favorites-label"]
        },
        "menu-behave" : {
            "type" : "section",
            "title" : "Behavior",
            "keys" : ["enable-autoscroll", "search-filesystem"]
        },
        "sidebar-layout" : {
            "type" : "section",
            "title" : "Sidebar Layout",
            "keys" : ["show-sidebar", "user-box-layout", "quicklauncher-layout"]
        },
        "quitmenu-layout" : {
            "type" : "section",
            "title" : "Quit Menu Layout",
            "keys" : ["show-shutdown-menu", "shutdown-menu-layout", "shutdown-label"]
        },
        "custom-quicklauncher" : {
            "type" : "section",
            "title" : "Custom Quicklauncher",
            "keys" : ["quicklauncher-0-checkbox", "quicklauncher-0-label", "quicklauncher-0-icon", "quicklauncher-0-command",
                      "quicklauncher-1-checkbox", "quicklauncher-1-label", "quicklauncher-1-icon", "quicklauncher-1-command",
                      "quicklauncher-2-checkbox", "quicklauncher-2-label", "quicklauncher-2-icon", "quicklauncher-2-command",
                      "quicklauncher-3-checkbox", "quicklauncher-3-label", "quicklauncher-3-icon", "quicklauncher-3-command",
                      "quicklauncher-4-checkbox", "quicklauncher-4-label", "quicklauncher-4-icon", "quicklauncher-4-command",
                      "quicklauncher-5-checkbox", "quicklauncher-5-label", "quicklauncher-5-icon", "quicklauncher-5-command",
                      "quicklauncher-6-checkbox", "quicklauncher-6-label", "quicklauncher-6-icon", "quicklauncher-6-command",
                      "quicklauncher-7-checkbox", "quicklauncher-7-label", "quicklauncher-7-icon", "quicklauncher-7-command",
                      "quicklauncher-8-checkbox", "quicklauncher-8-label", "quicklauncher-8-icon", "quicklauncher-8-command",
                      "quicklauncher-9-checkbox", "quicklauncher-9-label", "quicklauncher-9-icon", "quicklauncher-9-command",
                      "quicklauncher-10-checkbox", "quicklauncher-10-label", "quicklauncher-10-icon", "quicklauncher-10-command",
                      "quicklauncher-11-checkbox", "quicklauncher-11-label", "quicklauncher-11-icon", "quicklauncher-11-command",
                      "quicklauncher-12-checkbox", "quicklauncher-12-label", "quicklauncher-12-icon", "quicklauncher-12-command",
                      "quicklauncher-13-checkbox", "quicklauncher-13-label", "quicklauncher-13-icon", "quicklauncher-13-command",
                      "quicklauncher-14-checkbox", "quicklauncher-14-label", "quicklauncher-14-icon", "quicklauncher-14-command",
                      "quicklauncher-15-checkbox", "quicklauncher-15-label", "quicklauncher-15-icon", "quicklauncher-15-command",
                      "quicklauncher-16-checkbox", "quicklauncher-16-label", "quicklauncher-16-icon", "quicklauncher-16-command",
                      "quicklauncher-17-checkbox", "quicklauncher-17-label", "quicklauncher-17-icon", "quicklauncher-17-command",
