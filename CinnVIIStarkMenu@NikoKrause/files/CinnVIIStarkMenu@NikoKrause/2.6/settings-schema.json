    "type": "spinbutton",
    "default" : 0,
    "min" : 0,
    "max" : 1000,
    "step" : 50,
    "units" : "milliseconds",
    "description" : "Category hover delay",
    "tooltip" : "Delay between switching categories"
 },
 "enable-animation" : {
    "type": "checkbox",
    "default": false,
    "description": "Use menu animations",
    "tooltip": "Allow the menu to animate on open and close"
 },
