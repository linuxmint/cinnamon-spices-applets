{
 "head-menu-layout" : {
    "type" : "header",
    "description" : "Stark-Menu/MATE-Menu"
 },
 "menu-layout" : {
    "type": "combobox",
    "default": "stark-menu",
    "options": {
	    "Stark-Menu": "stark-menu",
	    "MATE-Menu": "mate-menu"
     },
    "description" : "Menu layout",
    "tooltip" : "Choose between Stark-Menu and MATE-Menu layout"
 },
 "separator-settings": {
    "type": "separator"
 },
 "head-settings" : {
    "type" : "header",
    "description" : "General Settings"
 },
