msgid "Bytes read"
msgstr "Octets lus"

#. settings-schema.json->column_type_sort->options
msgid "Bytes written"
msgstr "Octets écrits"

#. settings-schema.json->column_type_sort->options
msgid "Other"
msgstr "Autre"

