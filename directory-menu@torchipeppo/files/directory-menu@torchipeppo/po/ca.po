#: popup_menu.py:71
msgid "Open Folder"
msgstr "Obrir directori"

#: popup_menu.py:76
msgid "Open in Terminal"
msgstr "Obrir al Terminal"

#. metadata.json->name
msgid "Directory Menu"
msgstr "Menú de directoris"

#. metadata.json->description
msgid ""
"Quickly navigate through directories on your system using a dropdown menu."
msgstr ""
"Navegueu ràpidament entre directoris del vostre sistema de fitxers "
"utilitzant un menú desplegable."

#. settings-schema.json->starting-uri->description
msgid "Base Directory"
msgstr "Directori base"

#. settings-schema.json->appearance-section->description
msgid "Appearance"
msgstr "Aparença"

#. settings-schema.json->icon-name->description
msgid "Icon (hover here for info!)"
msgstr "Icona (passeu el cursor per aquí per més informació!)"

#. settings-schema.json->icon-name->tooltip
msgid "Must be an icon name (no path), and should be symbolic."
msgstr "Ha de ser el nom d'una icona (no una ruta) i ha de ser simbòlic."

#. settings-schema.json->tooltip->description
msgid "Tooltip Text"
msgstr "Text de la informació d'eines"

#. settings-schema.json->label->description
msgid "Panel Text"
msgstr "Text del tauler"

#. settings-schema.json->behavior-section->description
msgid "Behavior"
msgstr "Comportament"

#. settings-schema.json->show-hidden->description
msgid "Include hidden files"
msgstr "Inclou els fitxers ocults"

#. settings-schema.json->order-by->options
msgid "Name"
msgstr "Nom"

#. settings-schema.json->order-by->options
msgid "Date modified"
msgstr "Data de modificació"

#. settings-schema.json->order-by->description
msgid "Order by..."
msgstr "Ordenar per..."

#. settings-schema.json->favorites-first->description
msgid "Show favorite items first"
msgstr "Mostrar els elements preferits primer"

#. settings-schema.json->pinned-first->description
msgid "Show pinned items first"
msgstr "Mostrar els elements fixats primer"

#. settings-schema.json->limit-characters->description
msgid "Limit the Maximum Number of Characters on the Submenus"
msgstr "Limita el número de caràcters màxim dels submenús"

#. settings-schema.json->character-limit->description
msgid "Character Limit"
msgstr "Límit de caràcters"

#. settings-schema.json->character-limit->tooltip
msgid ""
"(Approximately) Limit the number of characters of each entry.\n"
"Useful if you think the menu is too wide."
msgstr ""
"(Aproximadament) Limita el número de caràcters de cada element.\n"
"Útil si penseu que el menú és massa ample."

#. settings-schema.json->keybindings-section->description
msgid "Keybindings"
msgstr "Dreceres del teclat"

#. settings-schema.json->show-menu->description
msgid "Show menu"
msgstr "Mostrar el menú"

#. settings-schema.json->show-menu->tooltip
msgid "Set keybinding(s) to show the menu."
msgstr "Dreceres del teclat per mostrar el menú."
