#: applet.js:20
msgid "Currently preventing screensaver"
msgstr "Actualmente impidiendo el protector de pantalla"

#: applet.js:21
msgid "Currently allowing screensaver"
msgstr "Actualmente permite el protector de pantalla"

#: applet.js:100
msgid "Screensaver settings"
msgstr "Configuración del protector de pantalla"

#. settings-schema.json->inhibit-at-startup->description
#: applet.js:104
msgid "Inhibit screensaver at startup"
msgstr "Inhibir el protector de pantalla al arrancar"

#. metadata.json->description
msgid "Click to inhibit the screensaver, click again to remove inhibition"
msgstr ""
"Haga clic para inhibir el protector de pantalla, haga clic de nuevo para "
"eliminar la inhibición"

#. metadata.json->name
msgid "Screensaver Inhibitor"
msgstr "Inhibidor del protector de pantalla"

#. settings-schema.json->head->description
msgid "Icons"
msgstr "Iconos"

#. settings-schema.json->off-icon->description
msgid "Power management allowed icon:"
msgstr "Icono de gestión de energía permitida:"

#. settings-schema.json->off-icon->tooltip
#. settings-schema.json->on-icon->tooltip
msgid "Select an icon file, or you can type an icon name into the entry box"
msgstr ""
"Seleccione un archivo de iconos, o puede escribir un nombre de icono en el "
"cuadro de entrada"

#. settings-schema.json->on-icon->description
msgid "Power management prevented icon:"
msgstr "Icono de gestión de energía impedida:"

#. settings-schema.json->reset-icons->description
msgid "Reset to default icons"
msgstr "Restablecer los iconos por defecto"

#. settings-schema.json->head1->description
msgid "Keybinding"
msgstr "Combinación de teclas"

#. settings-schema.json->keybinding->description
msgid "Set the keybinding for screensaver inhibit"
msgstr ""
"Establecer la combinación de teclas para permitir la inhibición del "
"protector de pantalla"

#. settings-schema.json->head2->description
msgid "Startup"
msgstr "Inicio"

#~ msgid ""
#~ "(More precisely, 10 seconds after startup, to ensure that the session is "
#~ "correctly launched)."
#~ msgstr ""
#~ "(Más concretamente, 10 segundos después del inicio, para garantizar que "
#~ "la sesión se inicia correctamente)."
