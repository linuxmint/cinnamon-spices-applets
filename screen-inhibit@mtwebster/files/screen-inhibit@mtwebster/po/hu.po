#: applet.js:21
msgid "Currently preventing screensaver"
msgstr "Jelenleg megakadályozza a képernyővédőt"

#: applet.js:22
msgid "Currently allowing screensaver"
msgstr "Jelenleg engedélyezi a képernyővédőt"

#: applet.js:123
msgid "Screensaver settings"
msgstr "Képernyővédő beállításai"

#. settings-schema.json->inhibit-at-startup->description
#: applet.js:127
msgid "Inhibit screensaver at startup"
msgstr "Képernyőkímélő letiltása indításkor"

#. metadata.json->description
msgid "Click to inhibit the screensaver, click again to remove inhibition"
msgstr ""
"Kattintson a képernyővédő gátlásához, kattintson ismét a gátlás "
"eltávolításához"

#. metadata.json->name
msgid "Screensaver Inhibitor"
msgstr "Képernyővédő gátló"

#. settings-schema.json->head->description
msgid "Icons"
msgstr "Ikonok"

#. settings-schema.json->off-icon->description
msgid "Power management allowed icon:"
msgstr "Az energiagazdálkodás engedélyezte az ikont:"

#. settings-schema.json->off-icon->tooltip
#. settings-schema.json->on-icon->tooltip
msgid "Select an icon file, or you can type an icon name into the entry box"
msgstr "Válasszon egy ikonfájlt, vagy beírhat egy ikonnevet a beviteli mezőbe"

#. settings-schema.json->on-icon->description
msgid "Power management prevented icon:"
msgstr "Az energiagazdálkodás megakadályozta az ikont:"

#. settings-schema.json->reset-icons->description
msgid "Reset to default icons"
msgstr "Alapértelmezett ikonok visszaállítása"

#. settings-schema.json->head1->description
msgid "Keybinding"
msgstr "Billentyűparancsot"

#. settings-schema.json->keybinding->description
msgid "Set the keybinding for screensaver inhibit"
msgstr "Állítsa be a billentyűparancsot a képernyővédő gátlásához"

#. settings-schema.json->head2->description
msgid "Startup"
msgstr "Indítás"

#. settings-schema.json->head3->description
msgid "Way to block the screensaver"
msgstr "A képernyővédő blokkolásának módja"

#. settings-schema.json->locklabel->description
msgid "(Avoid using aggressive ways, if possible.)"
msgstr "(Ha lehetséges, kerülje az agresszív módszerek használatát.)"

#. settings-schema.json->locktype->description
msgid "Blocking mode"
msgstr "Blokkoló mód"

#. settings-schema.json->locktype->options
msgid "Normal"
msgstr "Normál"

#. settings-schema.json->locktype->options
msgid "Aggressive"
msgstr "Agresszív"

#. settings-schema.json->locktype->options
msgid "Super aggressive"
msgstr "Szuper agresszív"

#. settings-schema.json->lockinterval->description
msgid "Blocking interval"
msgstr "Blokkolási intervallum"

#. settings-schema.json->lockinterval->units
msgid "minutes"
msgstr "perc"
