{
  "layoutSB": {
    "type": "layout",
    "pages": ["page-providers", "page-options"],
    "page-providers": {
      "type": "page",
      "title": "Providers",
      "sections": ["section-providers"]
    },
    "page-options": {
      "type": "page",
      "title": "Options",
      "sections": ["section-options", "section-colors"]
    },
    "section-providers": {
      "type": "section",
      "title": "Providers",
      "keys": ["providers", "providers-help"]
    },
    "section-options": {
      "type": "section",
      "title": "Options",
      "keys": ["show-provider", "reset-after-search"]
    },
    "section-colors": {
      "type": "section",
      "title": "Search-box colors",
      "keys": ["background-color", "text-color"]
    }
  },
  "header-provider": {
    "type": "header",
    "description": "Search Provider Settings"
  },
  "show-provider": {
    "type": "switch",
    "default": true,
    "description": "Show provider",
    "tooltip": "Show selected provider in the panel"
  },
  "reset-after-search": {
    "type": "switch",
    "default": true,
    "description": "Reset query after search",
    "tooltip": "Reset query after search? This may not be useful if you want to be able to perform the same query on other search engines afterwards."
  },
  "header-providers": {
    "type": "header",
    "description": "Search Engines"
  },
  "providers": {
    "type": "list",
    "description": "Providers",
    "height": 400,
    "columns": [
      {
        "id": "show",
        "title": "Show",
        "type": "boolean",
        "align": 0.5,
        "default": true
      },
      {
        "id": "name",
        "title": "Name",
        "type": "string",
        "expand-width": false,
        "align": 0.0,
        "default": ""
      },
      {
        "id": "short",
        "title": "Short name\n(3 lower-case letters)",
        "type": "string",
        "expand-width": false,
        "align": 0.0,
        "default": ""
      },
      {
        "id": "url",
        "title": "Query URL",
        "type": "string",
        "expand-width": true,
        "align": 0.0,
        "default": ""
      }
    ],
    "default": [
      {
        "show": true,
        "name": "Ask",
        "short": "ask",
        "url": "https://www.ask.com/web?q="
      },
      {
        "show": true,
        "name": "Bing",
        "short": "bng",
        "url": "https://www.bing.com/search?q="
      },
      {
        "show": true,
        "name": "DuckDuckGo",
        "short": "ddg",
        "url": "https://duckduckgo.com/?q="
      },
      {
        "show": true,
        "name": "Google",
        "short": "ggl",
        "url": "https://google.com/search?q="
      },
      {
        "show": true,
        "name": "Lilo",
        "short": "llo",
        "url": "https://search.lilo.org/?plugin=lilose&q="
      },
      {
        "show": true,
        "name": "RefSeek",
        "short": "rsk",
        "url": "https://www.refseek.com/search?q="
      },
      {
        "show": true,
        "name": "Yahoo",
        "short": "yoo",
        "url": "https://search.yahoo.com/search?p="
      },
      {
        "show": true,
        "name": "WorldCat",
        "short": "wca",
        "url": "https://search.worldcat.org/search?q="
      },
      {
        "show": true,
        "name": "Springer Link",
        "short": "spg",
        "url": "https://link.springer.com/search?query="
      },
      {
        "show": true,
        "name": "Bioline",
        "short": "bln",
        "url": "https://www.bioline.org.br/search?search_target=all_pub&search_string="
      },
      {
        "show": true,
        "name": "Pdf Drive",
        "short": "pdd",
        "url": "https://www.pdfdrive.com/search?q="
      },
      {
        "show": true,
        "name": "BASE",
        "short": "bse",
        "url": "https://www.base-search.net/Search/Results?lookfor="
      }
    ],
    "tooltip": "If your query begins with the 3 letters of the short name, it will be processed by the relevant search engine.\nExample: ddg linux mint iso"
  },
  "providers-help": {
    "type": "label",
    "description": "Only selected providers will be displayed in the context menu, so you can select your default provider.\nIf you deselect the default provider, the first selected becomes the default provider.\nIf no provider is selected, the default provider is Google."
  },
  "selected-provider": {
    "type": "generic",
    "default": "Google"
  },
  "use-custom-provider": {
    "type": "generic",
    "default": false
  },
  "custom-provider-label": {
    "type": "generic",
    "default": ""
  },
  "custom-provider-url": {
    "type": "generic",
    "default": ""
  },
  "header-keybinding": {
    "type": "header",
    "description": "Keybinding"
  },
  "custom-keybinding": {
    "type": "keybinding",
    "description": "Keybinding, which opens the search box",
    "default": "<Primary><Alt>f"
  },
  "header-colors": {
    "type": "header",
    "description": "Search-box colors"
  },
  "background-color": {
    "type": "colorchooser",
    "description": "Background color",
    "default": "rgb(222,221,218)"
  },
  "text-color": {
    "type": "colorchooser",
    "description": "Text color",
    "default": "rgb(36,31,49)"
  }
}
