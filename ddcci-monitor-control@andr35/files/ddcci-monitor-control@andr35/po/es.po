#. metadata.json->description
#. applet.js:41
msgid "Adjust external monitor brightness and contrast"
msgstr "Ajustar el brillo y el contraste del monitor externo"

#. metadata.json->name
msgid "DDC/CI Monitor Brightness"
msgstr "Brillo del monitor DDC/CI"

#. settings-schema.json->brightness-default-label-1->description
msgid "\"Default brightness 1\" button label"
msgstr "Etiqueta del botón \"Brillo por defecto 1\""

#. settings-schema.json->brightness-default-label-1->tooltip
msgid "For example \"Daily Brightness\""
msgstr "Por ejemplo, \"Brillo de día\""

#. settings-schema.json->brightness-default-value-1->description
msgid "\"Default brightness 1\" value"
msgstr "Valor \"Brillo predeterminado 1\""

#. settings-schema.json->brightness-default-value-1->tooltip
