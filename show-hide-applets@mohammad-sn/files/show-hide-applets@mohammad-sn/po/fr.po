#: applet.js:51
msgid "Panel Edit mode"
msgstr "Mode d'édition du panneau"

#: applet.js:57
msgid "Add applets to the panel"
msgstr "Ajouter des applets au panneau"

#. show-hide-applets@mohammad-sn->metadata.json->description
msgid ""
"hide a few applet to have a neater panel, and make them show up again by a "
"click"
msgstr ""
"cachez quelques applet pour avoir un panneau plus net, et faites-les "
"réapparaître d'un simple clic"

#. show-hide-applets@mohammad-sn->metadata.json->name
msgid "Drawer (show/hide applets)"
msgstr "Tiroir (afficher/masquer les applets)"

#. show-hide-applets@mohammad-sn->settings-
#. schema.json->hoveractivateshide->description
msgid "Hide on hovering again"
msgstr "Cacher en survolant à nouveau"

#. show-hide-applets@mohammad-sn->settings-
#. schema.json->hoveractivateshide->tooltip
msgid "toggle visibility of applets on mouse hover"
msgstr "basculer la visibilité des applets au survol de la souris"

#. show-hide-applets@mohammad-sn->settings-
#. schema.json->autohiderstime->description
msgid "Reshowers' autohide delay: "
msgstr "Délai de masquage automatique des applets qui se ré-affichent : "

#. show-hide-applets@mohammad-sn->settings-schema.json->autohiderstime->units
#. show-hide-applets@mohammad-sn->settings-schema.json->hidetime->units
msgid "seconds"
msgstr "secondes"

#. show-hide-applets@mohammad-sn->settings-
#. schema.json->disablestarttimeautohide->description
msgid "Disable start time autohide"
msgstr "Désactiver  l'heure de démarrage du masquage automatique"

#. show-hide-applets@mohammad-sn->settings-
#. schema.json->disablestarttimeautohide->tooltip
msgid "if not in autohide mode"
msgstr "si pas en mode masquage automatique"

#. show-hide-applets@mohammad-sn->settings-schema.json->head->description
msgid "Settings for show-hide-applets@mohammad-sn"
msgstr "Préférences pour show-hide-applets@mohammad-sn"

#. show-hide-applets@mohammad-sn->settings-schema.json->autohide->description
msgid "Autohide"
msgstr "Masquage automatique"

#. show-hide-applets@mohammad-sn->settings-schema.json->autohide->tooltip
msgid ""
"check if you want to hide applets automatically, unless you should click on "
"applet to do so"
msgstr ""
"cochez si vous souhaitez masquer automatiquement les applets, sauf si vous "
"devez cliquer sur l'applet pour le faire"

#. show-hide-applets@mohammad-sn->settings-
#. schema.json->autohiders->description
msgid "Autohide reshowing applets"
msgstr "Masquer automatiquement les applets qui se ré-affichent"

#. show-hide-applets@mohammad-sn->settings-schema.json->hovertime->description
msgid "Hover activates/hides after "
msgstr "Le survol active/cache après "

#. show-hide-applets@mohammad-sn->settings-schema.json->hovertime->units
msgid "miliseconds"
msgstr "milisecondes"

#. show-hide-applets@mohammad-sn->settings-schema.json->hidetime->description
msgid "Autohide after "
msgstr "Masquer automatiquement après "

#. show-hide-applets@mohammad-sn->settings-
#. schema.json->hoveractivates->description
msgid "Activate on hover"
msgstr "Activer au survol"

#. show-hide-applets@mohammad-sn->settings-
#. schema.json->hoveractivates->tooltip
msgid "show applets on mouse hover"
msgstr "afficher les applets au survol de la souris"
