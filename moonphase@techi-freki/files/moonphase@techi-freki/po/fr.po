#. metadata.json->name
msgid "Moon Phase"
msgstr "Phase de la Lune"

#. metadata.json->description
msgid "A Cinnamon applet that displays up-to-date moon related information."
msgstr ""
"Une applet Cinnamon qui affiche des informations actualisées sur la lune."

#. settings-schema.json->page-settings->title
msgid "Settings"
msgstr "Paramètres"

#. settings-schema.json->section-geolocation->title
msgid "Geolocation Settings"
msgstr "Paramètres de géolocalisation"

#. settings-schema.json->section-general->title
msgid "General Settings"
msgstr "Paramètres généraux"

#. settings-schema.json->section-update->title
msgid "Update Settings"
msgstr "Paramètres de mise à jour"

#. settings-schema.json->enableGeolocation->description
msgid "Enable geolocation features"
msgstr "Activer les fonctions de géolocalisation"

#. settings-schema.json->latitude->description
msgid "Latitude"
msgstr "Latitude"

#. settings-schema.json->longitude->description
msgid "Longitude"
msgstr "Longitude"

