#. applet.js:102
msgid "desktop"
msgstr "màn hình nền"

#. applet.js:316
msgid "No open windows"
msgstr "Không có cửa sổ nào đang mở"

#. applet.js:326
msgid "Expo"
msgstr "Expo"

#. metadata.json->description
msgid ""
"click to show/hide desktop, scroll to switch workspace and windows quick "
"list by right click"
msgstr ""
"nhấp để hiện/ẩn màn hình nền, cuộn để chuyển workspace và danh sách nhanh "
"cửa sổ bằng chuột phải"

#. metadata.json->name
msgid "Show desktop ++"
msgstr "Hiện màn hình nền ++"

#. settings-schema.json->head->description
msgid "Appearance"
msgstr "Giao diện"

#. settings-schema.json->icon-name->description
msgid "The icon to use in the panel:"
msgstr "Biểu tượng sử dụng trên panel:"

#. settings-schema.json->icon-name->tooltip
msgid ""
"Select an icon file, or you can type an icon name into the entry box, for "
"example: desktop"
msgstr ""
"Chọn một tập tin biểu tượng, hoặc bạn có thể nhập tên biểu tượng vào ô nhập, "
"ví dụ: desktop"

#. settings-schema.json->width->units
msgid "pixels"
msgstr "pixel"

#. settings-schema.json->width->description
msgid "Applet width in the panel:"
msgstr "Chiều rộng applet trên panel:"

#. settings-schema.json->width->tooltip
msgid ""
"Increase or decrease this spinner value to make the applet wider or narrower "
"in the panel"
msgstr ""
"Tăng hoặc giảm giá trị này để làm applet rộng hơn hoặc hẹp hơn trên panel"

#. settings-schema.json->color->description
msgid "The background color for the applet hover:"
msgstr "Màu nền cho applet khi di chuột:"

#. settings-schema.json->color->tooltip
msgid "Click the button to select a new background color for applet"
msgstr "Nhấp nút để chọn màu nền mới cho applet"

#. settings-schema.json->highlight->description
msgid "Highlight applet next time it is added to the panel"
msgstr "Làm nổi bật applet lần tiếp theo khi thêm vào panel"

#. settings-schema.json->head1->description
msgid "Menu options"
msgstr "Tùy chọn menu"

#. settings-schema.json->closebuttons->description
msgid "Show close buttons"
msgstr "Hiển thị nút đóng"

#. settings-schema.json->closeallbuttons->description
msgid "Show close all windows buttons for workspaces"
msgstr "Hiển thị nút đóng tất cả cửa sổ cho workspaces"

#. settings-schema.json->SHOW-MENU-ON-CLOSE->description
msgid "Keep menu open while closing windows"
msgstr "Giữ menu mở trong khi đóng cửa sổ"

#. settings-schema.json->head2->description
msgid "Peek at desktop"
msgstr "Xem trước màn hình nền"

#. settings-schema.json->peekatdesktop->description
msgid "Opacify windows on hover"
msgstr "Làm mờ cửa sổ khi di chuột"

#. settings-schema.json->peekatdesktop->tooltip
msgid "Take a glance at your desktop and desklets."
msgstr "Liếc nhìn màn hình nền và desklets của bạn."

#. settings-schema.json->alsoopacifyskiptaskbar->description
msgid "Opacify the icons on the desktop"
msgstr "Làm mờ các biểu tượng trên màn hình nền"

#. settings-schema.json->alsoopacifydesklets->description
msgid "Opacify desklets"
msgstr "Làm mờ desklets"

#. settings-schema.json->peekopacity->description
msgid "Opacity on hover:"
msgstr "Độ mờ khi di chuột:"

#. settings-schema.json->blur->description
msgid "Blur windows on hover"
msgstr "Làm nhòe cửa sổ khi di chuột"
