#. applet.js:18
msgid "GPU Temperature"
msgstr "Temperatura de la GPU"

#. applet.js:34
msgid "Nvidia GPU Temperature Indicator: Error"
msgstr "Indicador de temperatura de la GPU Nvidia: Error"

#. applet.js:34
msgid "It seems that nvidia-smi is not installed."
msgstr "Parece que nvidia-smi no está instalado."

#. metadata.json->name
msgid "Nvidia GPU Temperature Indicator"
msgstr "Indicador de temperatura de la GPU Nvidia"

#. metadata.json->description
msgid "Shows Nvidia GPU Temperature"
msgstr "Muestra las temperaturas de la GPU Nvidia"

#. settings-schema.json->interval->units
msgid "ms"
msgstr "ms"

#. settings-schema.json->interval->description
msgid "Update interval"
msgstr "Intervalo de actualización"

#. settings-schema.json->interval->tooltip
msgid "How often to update the temperature value."
msgstr "Frecuencia con la que se actualiza el valor de la temperatura."

#. settings-schema.json->use-fahrenheit->description
msgid "Show temperature in Fahrenheit"
msgstr "Muestra la temperatura en grados Fahrenheit"

#. settings-schema.json->use-fahrenheit->tooltip
msgid "Whether or not show the temperature in Fahrenheit instead of Celsius."
msgstr ""
"Mostrar o no la temperatura en grados Fahrenheit en lugar de Celsius."

#. settings-schema.json->show-unit->description
msgid "Show unit"
msgstr "Mostrar unidad"

#. settings-schema.json->show-unit->tooltip
msgid "Whether or not show the unit after the temperature value."
msgstr "Mostrar o no la unidad después del valor de temperatura."

#. settings-schema.json->show-unit-letter->description
msgid "Show unit letter"
msgstr "Mostrar letra de unidad"

#. settings-schema.json->show-unit-letter->tooltip
msgid "Whether or not show the unit letter (C or F)."
msgstr "Mostrar o no la letra de la unidad (C o F)."
