#: applet.js:62
msgid "Launch Radeontop"
msgstr "Avvia Radeontop"

#. metadata.json->name
msgid "AMD GPU Monitor"
msgstr "Monitora GPU AMD"

#. metadata.json->description
msgid "Monitor AMD GPU statistics, requires radeontop"
msgstr "Monitora le statistiche delle GPU AMD, richiede radeontop"

#. settings-schema.json->head->description
msgid "Settings"
msgstr "Impostazioni"

#. settings-schema.json->display-style->description
msgid "Display style"
msgstr "Stile di visualizzazione"

#. settings-schema.json->display-style->options
msgid "Column"
msgstr "Colonna"

#. settings-schema.json->display-style->options
msgid "Side by side"
msgstr "Fianco a fianco"

#. settings-schema.json->display-style->options
msgid "GPU only"
msgstr "Solo GPU"

#. settings-schema.json->display-style->options
msgid "VRAM only"
msgstr "Solo VRAM"

#. settings-schema.json->use-compact-label->description
msgid "Use compact label style"
msgstr "Utilizza lo stile etichetta compatta"

#. settings-schema.json->use-compact-label->tooltip
msgid "Show only the first character of each label. Example: G for GPU"
msgstr ""
"Mostra solo il primo carattere di ciascuna etichetta. Esempio: G per GPU"

