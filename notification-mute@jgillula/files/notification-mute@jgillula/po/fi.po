#: applet.js:21
msgid "Currently preventing notifications"
msgstr "Tällä hetkellä estetään ilmoitukset"

#: applet.js:22
msgid "Currently allowing notifications"
msgstr "Tällä hetkellä sallitaan ilmoitukset"

#: applet.js:92
msgid "Notification settings"
msgstr "Ilmoitusten asetukset"

#. metadata.json->name
msgid "Notification Mute"
msgstr "Ilmoituksen mykistys"

#. metadata.json->description
msgid "Click to mute notifications, click again to unmute"
msgstr "Mykistä ilmoitukset napsauttamalla ja poista mykistys napsauttamalla uudelleen"

#. settings-schema.json->head->description
msgid "Icons"
msgstr "Kuvakkeet"

#. settings-schema.json->use-custom-off-icon->description
msgid "Use a custom icon for when notifications are allowed."
msgstr "Käytä kuvaketta, kun ilmoitukset ovat sallittuja."

#. settings-schema.json->off-icon->description
msgid "Notifications allowed icon:"
msgstr "Ilmoitukset sallittu kuvake:"

#. settings-schema.json->off-icon->tooltip
#. settings-schema.json->on-icon->tooltip
msgid "Select an icon file, or you can type an icon name into the entry box"
msgstr "Valitse kuvakeen tiedosto tai voit myös kirjoittaa nimen tekstiruutuun"

#. settings-schema.json->use-custom-on-icon->description
msgid "Use a custom icon for when notifications are muted."
msgstr "Käytä kuvaketta, kun ilmoitukset ovat sallittu."

#. settings-schema.json->on-icon->description
msgid "Notifications muted icon:"
msgstr "Ilmoitukset estetty kuvake:"

#. settings-schema.json->head1->description
msgid "Keybinding"
msgstr "Pikanäppäin"

#. settings-schema.json->keybinding->description
msgid "Set the keybinding for toggling notification mute"
msgstr "Aseta näppäinyhdistelmä mykistyksen vaihtamiseksi"
