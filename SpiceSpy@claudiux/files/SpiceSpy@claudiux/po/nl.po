#. settings-schema.json->uuid-list->columns->title
msgid "UUID"
msgstr "UUID"

#. settings-schema.json->sort-by->options
msgid "Score (bottom-up)"
msgstr "Score (oplopend)"

#. settings-schema.json->sort-by->options
msgid "Score (top-down)"
