#: 2.8/applet.js:673
msgid "Network Connections"
msgstr "Mrežna povezivanja"

#: 2.8/applet.js:715
msgid "Connections"
msgstr "Povezivanja"

#: 2.8/applet.js:771
msgid "Click to disconnect from VPN"
msgstr "Klikni za prekidanje VPN povezivanja"

#: 2.8/applet.js:793
msgid "Connected"
msgstr "Povezano"

#: 2.8/applet.js:800
msgid "Disconnected"
msgstr "Nepovezano"

#: 2.8/applet.js:802
