msgid "0°"
msgstr "0°"

#. settings-schema.json->keyAnimationRotation->options
msgid "90°"
msgstr "90°"

#. settings-schema.json->keyAnimationRotation->options
msgid "180°"
msgstr "180°"

#. settings-schema.json->keyAnimationRotation->options
msgid "270°"
msgstr "270°"

