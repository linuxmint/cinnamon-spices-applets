msgid "Log Level"
msgstr "Niveau de log"

#. settings-schema.json->keyLogLevel->options
msgid "Info"
msgstr "Info"

#. settings-schema.json->keyLogLevel->options
msgid "Debug"
msgstr "Débogage"

#. settings-schema.json->keyLogLevel->options
msgid "Warning"
