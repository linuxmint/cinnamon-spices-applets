#: applet.js:26
msgid "%l:%M %p %Z"
msgstr "%l:%M %p %Z"

#: tzclock_settings.py:66
msgid "Timezone"
msgstr "Tidszone"

#: tzclock_settings.py:207
msgid "Region"
msgstr "Område"

#: tzclock_settings.py:210
msgid "City"
msgstr "By"

#. settings-schema.json->tzclock-settings-header->description
msgid "Timezone Clock Settings"
msgstr "Indstillinger for tidszone"

#. settings-schema.json->displayed-timezone->description
msgid "Timezone to display"
msgstr "Tidszone der skal vises"

#. settings-schema.json->displayed-timezone->tooltip
msgid "Enter a valid timezone name to display."
msgstr "Indtast et gyldigt navn på den tidszone, der skal vises"

#. settings-schema.json->tzclocks-settings-button->description
msgid "Select timezone on map"
msgstr "Vælg tidszone på kort"

#. settings-schema.json->tzclocks-settings-button->tooltip
msgid "Click this button to select timezone on map."
msgstr "Klik på denne knap for at vælge tidszone på et kort."

#. settings-schema.json->browse-timezones-button->description
msgid "Browse valid timezone values by country"
msgstr "Gennemse tidszoner efter land"

#. settings-schema.json->browse-timezones-button->tooltip
msgid "Click this button to browse valid timezone values by country."
msgstr ""
"Klik på denne knap for at gennemse gyldige værdier for tidszoner sorteret "
"efter land."

#. settings-schema.json->clock-display->description
msgid "Clock Display Format"
msgstr "Urets visningsformat"

#. settings-schema.json->clock-use-24h->description
msgid "Display the time using a 24 hour clock"
msgstr "Vis tid i 24-timers format"

#. settings-schema.json->clock-use-24h->tooltip
msgid ""
"Check this to display the time using a 24 hour clock in the timezone clock "
"applet."
msgstr ""
"Aktivér denne for at vise tiden i 24-timers format i panelprogrammet."

#. settings-schema.json->clock-show-seconds->description
msgid "Display the time with seconds"
msgstr "Viser sekunderne"

#. settings-schema.json->clock-show-seconds->tooltip
msgid ""
"Check this to display the time with seconds in the timezone clock applet."
msgstr "Aktivér denne for at få vist sekunderne i panelprogrammet."

#. settings-schema.json->clock-show-date->description
msgid "Display the date"
msgstr "Vis datoen"

#. settings-schema.json->clock-show-date->tooltip
msgid "Check this to display the date in the timezone clock applet."
msgstr "Aktivér denne for at få vist datoen i panelprogrammet."

#. settings-schema.json->use-custom-format->description
msgid "Use a custom date format"
msgstr "Anvend et tilpasset datoformat"

#. settings-schema.json->use-custom-format->tooltip
msgid ""
"Check this to define a custom format for the date in the timezone clock "
"applet."
msgstr ""
"Aktivér denne for at angive et tilpasset format for datoen i "
"panelprogrammet."

#. settings-schema.json->custom-format->description
msgid "Date format:"
msgstr "Datoformat:"

#. settings-schema.json->custom-format->tooltip
msgid "Set your custom format here."
msgstr "Angiv dit tilpasset format her."

#. settings-schema.json->format-button->description
msgid "Show information on date format syntax"
msgstr "Vis information om syntaks for datoformat"

#. settings-schema.json->format-button->tooltip
msgid "Click this button to know more about the syntax for date formats."
msgstr "Tryk på denne knap for at lære mere om syntaksen for datoformater."

#. metadata.json->name
msgid "Timezone Clock"
msgstr "Tidszoneur"

#. metadata.json->description
msgid "A simple clock for displaying the current time in a timezone"
msgstr "Et simpelt ur til visning af den aktuelle tid i en tidszone"
