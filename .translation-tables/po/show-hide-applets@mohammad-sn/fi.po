#: applet.js:60
msgid "Panel Edit mode"
msgstr "Paneelin muokkaustila"

#: applet.js:66
msgid "Add applets to the panel"
msgstr "Lisää sovelmia paneeliin"

#. metadata.json->description
msgid ""
"hide a few applet to have a neater panel, and make them show up again by a "
"click"
msgstr ""
"Piilota osa sovelmista ja näet siistimmän paneelin. Saat ne uudelleen "
"näkyviin yhdellä napsautuksella"

#. metadata.json->name
msgid "Drawer (show/hide applets)"
msgstr "Drawer (näytä ja piilota paneelin sovelmia)"

#. settings-schema.json->head->description
msgid "Settings for show-hide-applets@mohammad-sn"
msgstr "Asetukset: show-hide-applets@mohammad-sn"

#. settings-schema.json->autohide->description
msgid "Autohide"
msgstr "Piilota automaattisesti"

#. settings-schema.json->autohide->tooltip
msgid ""
"check if you want to hide applets automatically, unless you should click on "
"applet to do so"
msgstr ""
"Haluatko piilottaa sovelmat automaattisesti? Jos ei niin sovelmaa tulee itse "
"napsauttaa hiirellä"

#. settings-schema.json->disablestarttimeautohide->description
msgid "Disable start time autohide"
msgstr "Poista käynnistyksessä automaattinen piilotus käytöstä"

#. settings-schema.json->disablestarttimeautohide->tooltip
msgid "if not in autohide mode"
msgstr "Jos ei niin piilotustila on automaattinen"

#. settings-schema.json->hidetime->units
#. settings-schema.json->autohiderstime->units
msgid "seconds"
msgstr "sekuntia"

#. settings-schema.json->hidetime->description
msgid "Autohide after "
msgstr "Piilota automaattisesti jälkeen "

#. settings-schema.json->hoveractivates->description
msgid "Activate on hover"
msgstr "Aktivoi osoittaminen"

#. settings-schema.json->hoveractivates->tooltip
msgid "show applets on mouse hover"
msgstr "Näyttää sovelmat osoittamalla hiirellä"

#. settings-schema.json->hoveractivateshide->description
msgid "Hide on hovering again"
msgstr "Piilota kun osoitat tätä sovelmaa"

#. settings-schema.json->hoveractivateshide->tooltip
msgid "toggle visibility of applets on mouse hover"
msgstr "Vaihtaa sovelmien näkyvyyttä osoittamalla hiirellä"

#. settings-schema.json->hovertime->units
msgid "miliseconds"
msgstr "millisekuntia"

#. settings-schema.json->hovertime->description
msgid "Hover activates/hides after "
msgstr "Osoitus aktivoi/piilottaa jälkeen"

#. settings-schema.json->autohiders->description
msgid "Autohide reshowing applets"
msgstr "Piilota uudelleen automaattisesti"

#. settings-schema.json->autohiderstime->description
msgid "Reshowers' autohide delay: "
msgstr "Piilotuksen viive uudelleen: "
