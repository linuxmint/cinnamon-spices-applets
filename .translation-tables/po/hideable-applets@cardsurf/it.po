msgid "Filepath"
msgstr "Percorso file"

#. settings-schema.json->gui_icon_type->options
msgid "Symbolic"
msgstr "Simbolica"

#. settings-schema.json->gui_icon_type->tooltip
msgid "A type of an icon displayed on the panel"
msgstr "Un tipo di icona visualizzata sul pannello"

