#, javascript-format
msgid "%d second"
msgid_plural "%d seconds"
msgstr[0] "%d segundo"
msgstr[1] "%d segundos"

#: applet.js:1024
#, javascript-format
msgid "%s and %s"
msgstr "%s e %s"

#: applet.js:1048
