msgid "Pomodori %d, set %d running"
msgstr "Repetición %d, ciclo %d en marcha"

#: applet.js:289
msgid "Waiting to start"
msgstr "Esperando para empezar"

# notifications
#: applet.js:342
msgid "Pomodoro started"
msgstr "Pomodoro iniciado"

#: applet.js:407
msgid "Let's go to work!"
msgstr "¡A trabajar!"

#: applet.js:425
msgid "Take a short break"
msgstr "Tome un descanso corto"

#: applet.js:448
msgid "Take a long break"
msgstr "Tome un descanso largo"

#: applet.js:484
