msgid "Settings"
msgstr "Asetukset"

#. settings-schema.json->display-style->description
msgid "Display style"
msgstr "Näyttötyyli"

#. settings-schema.json->display-style->options
msgid "Column"
msgstr "Sarake"

#. settings-schema.json->display-style->options
msgid "Side by side"
msgstr "Rinnakkain"

#. settings-schema.json->display-style->options
msgid "GPU only"
msgstr "Vain GPU"

#. settings-schema.json->display-style->options
msgid "VRAM only"
