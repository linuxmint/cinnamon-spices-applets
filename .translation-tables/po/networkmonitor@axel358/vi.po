msgid "bytes per second"
msgstr "byte mỗi giây"

#. settings-schema.json->hide-umbral->description
msgid "Hide speed if network traffic is less than"
msgstr "Ẩn tốc độ nếu lưu lượng mạng nhỏ hơn"

#. settings-schema.json->hide-umbral->tooltip
msgid "Use a compact display style if network traffic is less than the specified amount"
msgstr "Sử dụng kiểu hiển thị gọn nếu lưu lượng mạng nhỏ hơn mức quy định"
