#: applet.js:32
msgid "Open System Monitor"
msgstr "開啟系統資源監視器"

#. metadata.json->name
msgid "Network Monitor"
msgstr "Network Monitor"

#. metadata.json->description
msgid "Monitor network traffic"
msgstr "監控網路流量"

#. settings-schema.json->head->description
msgid "Settings"
msgstr "設定"

#. settings-schema.json->display-style->description
msgid "Speed display style"
msgstr "速度顯示樣式"

#. settings-schema.json->display-style->options
msgid "Combined"
msgstr "合併顯示"

#. settings-schema.json->display-style->options
msgid "Column"
msgstr "直欄顯示"

#. settings-schema.json->display-style->options
msgid "Side by side"
msgstr "並排顯示"

#. settings-schema.json->display-style->options
msgid "Download only"
msgstr "僅顯示下載"

#. settings-schema.json->display-style->options
msgid "Upload only"
msgstr "僅顯示上傳"

#. settings-schema.json->display-style->tooltip
msgid "Whether to show download and upload speeds separately or combined"
msgstr "選擇分開或合併顯示下載與上傳速度"

#. settings-schema.json->refresh-interval->units
msgid "milliseconds"
msgstr "毫秒"

#. settings-schema.json->refresh-interval->description
msgid "Refresh interval"
msgstr "更新間隔"

#. settings-schema.json->refresh-interval->tooltip
msgid "Time between data refeshes"
msgstr "資料更新的時間間隔"

#. settings-schema.json->decimal-places->description
msgid "Decimal places"
msgstr "小數點位數"

#. settings-schema.json->decimal-places->tooltip
msgid "Number of decimal places to show"
msgstr "要顯示的小數點位數"

#. settings-schema.json->hide-umbral->units
msgid "bytes per second"
msgstr "位元組/秒"

#. settings-schema.json->hide-umbral->description
msgid "Hide speed if network traffic is less than"
msgstr "當網路流量低於指定值時隱藏速度"

#. settings-schema.json->hide-umbral->tooltip
msgid ""
"Use a compact display style if network traffic is less than the specified "
"amount"
msgstr "當網路流量低於指定值時，系統會使用精簡的顯示樣式"
