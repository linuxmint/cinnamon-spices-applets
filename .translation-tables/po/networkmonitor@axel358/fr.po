#. settings-schema.json->display-style->description
msgid "Speed display style"
msgstr "Style d'affichage de la vitesse"

#. settings-schema.json->display-style->options
msgid "Combined"
msgstr "Combinés"

#. settings-schema.json->display-style->options
msgid "Column"
msgstr "Colonne"

#. settings-schema.json->display-style->options
msgid "Side by side"
msgstr "Côte à côte"

#. settings-schema.json->display-style->options
msgid "Download only"
