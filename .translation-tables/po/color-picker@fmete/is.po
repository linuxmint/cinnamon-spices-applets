#: applet.js:80
msgid "Click here to pick a color"
msgstr "Smelltu hér til að plokka lit"

#: applet.js:136
#, javascript-format
msgid "Please install the '%s' package."
msgstr "Settu upp '%s' pakkann."

#: applet.js:158
#, javascript-format
msgid "Color code '%s' copied to clipboard."
msgstr "Litakóðinn '%s' afritaður á klippispjald."

#. color-picker@fmete->settings-schema.json->pick-notification->description
msgid "Show picked color code in a popup notification"
msgstr "Sýna valinn lit í sprettglugga"

#. color-picker@fmete->settings-schema.json->pick-notification->tooltip
msgid ""
"Show the picked color code, which has been copied to the clipboard, in a "
"popup notification."
msgstr ""
"Sýna valinn lit, sem hefur verið afritaður á klippispjaldið, í "
"tilkynningasprettglugga."

#. color-picker@fmete->settings-schema.json->head->description
msgid "Color Pick Settings"
msgstr "Stillingar litaplokkara"

#. color-picker@fmete->settings-schema.json->head1->description
msgid "Applet Settings"
msgstr "Stillingar smáforrits"

#. color-picker@fmete->settings-schema.json->combo-selection->description
msgid "Color code format"
msgstr "Snið litakóða"

#. color-picker@fmete->settings-schema.json->combo-selection->tooltip
msgid ""
"Select the format of the color code, which will be copied to the clipboard "
"by the color picker."
msgstr ""
"Veldu sniðið á litakóðanum, sem verður afritaður á klippispjaldið af "
"litaplokkaranum."

#. color-picker@fmete->settings-schema.json->combo-selection->options
msgid "HEX-Color: #XXXXXX"
msgstr "HEX-litur: #XXXXXX"

#. color-picker@fmete->settings-schema.json->combo-selection->options
msgid "RGB-Color: 250,250,250"
msgstr "RGB-litur: 250,250,250"

#. color-picker@fmete->settings-schema.json->combo-selection->options
msgid "HEX-Color: #xxxxxx"
msgstr "HEX-litur: #xxxxxx"

#. color-picker@fmete->settings-schema.json->combo-selection->options
msgid "RGB-Color: (250,250,250)"
msgstr "RGB-litur: (250,250,250)"

#. color-picker@fmete->settings-schema.json->icon-name->description
msgid "The icon to use in the panel"
msgstr "Táknmynd sem nota skal á skjástikunni"

#. color-picker@fmete->settings-schema.json->icon-name->tooltip
msgid "Select an icon file, or you can type an icon name into the entry box"
msgstr ""
"Veldu táknmyndaskrá, eða að þú getur slegið inn heiti táknmyndar í "
"inntaksreitinn"

#. color-picker@fmete->settings-schema.json->keybinding-test->description
msgid "Keybinding, which activates the color picker"
msgstr "Flýtilyklasamsetning, sem virkjar litaplokkarann"

#. color-picker@fmete->metadata.json->description
msgid ""
"Use the color picker to select a color on the screen. The color value (HEX "
"or RGB) will be copied to the clipboard."
msgstr ""
"Notaðu litaplokkarann til að velja lit á skjánum. Litgildið (HEX eða RGB) "
"verður afritað á klippispjaldið."

#. color-picker@fmete->metadata.json->name
msgid "Color Picker"
msgstr "Litaplokkari"
