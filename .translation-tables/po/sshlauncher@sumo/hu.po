#. metadata.json->name
#: applet.js:40 applet.js:56 applet.js:99 applet.js:206 applet.js:223
msgid "SSH Launcher"
msgstr "SSH indító"

#: applet.js:99
msgid "No suitable icon found, falling back to custom icon"
msgstr "Nem található megfelelő ikon, visszatérés az egyéni ikonra"

#: applet.js:105
msgid "Force Update from SSH config"
msgstr "Erőltetett frissítés az SSH konfigurációból"

#: applet.js:143
msgid "Background (-fN)"
msgstr "Háttér (-fN)"

#: applet.js:146
msgid "Forward X11 (-X)"
msgstr "Előre X11 (-X)"

#: applet.js:187
msgid "ERROR. "
msgstr "HIBA. "

#: applet.js:190
msgid "Edit SSH config"
msgstr "SSH konfiguráció szerkesztése"

#: applet.js:206
msgid ""
"Error: I can't open the terminal.\n"
"You need to set a default terminal first in Preferred Applications!\n"
"\n"
"(More help in Right Click->About->More Info)"
msgstr ""
"Hiba: Nem tudom megnyitni a terminált.\n"
"Először be kell állítania egy alapértelmezett terminált a Preferált "
"Alkalmazásokban!\n"
"\n"
"(További segítség a jobb egérgombbal-> Névjegy-> További információ)"

#: applet.js:223
msgid "Connection opened to "
msgstr "A kapcsolat megnyitva "

#: applet.js:223
msgid " using "
msgstr " használatban "

#. settings-schema.json->headAppearance->description
msgid "Appearance"
msgstr "Megjelenés"

#. settings-schema.json->themeIcon->description
msgid "Use icon from your icon theme"
msgstr "Ikon használata az ön ikontémájából"

#. settings-schema.json->symbolicIcon->description
msgid "Use symbolic icon"
msgstr "Szimbolikus ikon használata"

#. settings-schema.json->headFunc->description
msgid "Functionality"
msgstr "Funkcionalitás"

#. settings-schema.json->customCommand->description
msgid "Use custom command for opening terminal"
msgstr "Egyéni parancs használata a terminál megnyitásához"

#. settings-schema.json->customCommand->tooltip
msgid ""
"Usually it should end with -e so it will execute the supplied ssh command, "
"check your chosen terminal's manual."
msgstr ""
"Általában -e legyen a végződés, így végrehajtja a mellékelt ssh parancsot, "
"ellenőrizze az ön által használt terminál kézikönyvét."

#. metadata.json->description
msgid "An applet to connect to SSH hosts configured in ~/.ssh/config"
msgstr ""
"Egy kisalkalmazás, amellyel az ~/.ssh/config helyen beállított SSH "
"kiszolgálókhoz csatlakozhat"
