#. settings-schema.json->notification-enabled->description
msgid "Show Notifications"
msgstr "Hiện thông báo"

#. settings-schema.json->notification-enabled->tooltip
msgid "Show notification on application launch"
msgstr "Hiện thông báo khi khời chạy ứng dụng"

#. settings-schema.json->notification-text->description
msgid "Notification Text"
msgstr "Nội dung thông báo"

#. settings-schema.json->notification-text->tooltip
msgid "%s will be your application name"
msgstr "%s sẽ là tên ứng dụng của bạn"

#. settings-schema.json->hotkeys-enabled->description
msgid "Enable Hotkeys"
msgstr "Bật phím nóng"

#. settings-schema.json->hotkey-bindings->description
msgid "Show menu"
msgstr "Hiển thị menu"

#. settings-schema.json->hotkey-bindings->tooltip
msgid "Set keybinding(s) to show the menu"
msgstr "Đặt phím tắt để hiển thị menu"

#. settings-schema.json->menu-at-pointer->description
msgid "Menu at mouse position"
msgstr "Menu ở vị trí chuột"

#. settings-schema.json->menu-at-pointer->tooltip
msgid "Show the menu next to the cursor when triggered by hotkey"
msgstr "Hiển thị menu bên cạnh con trỏ khi được kích hoạt bằng phím nóng"

#. settings-schema.json->open-by-hover->description
msgid "Open menus by hovering over them"
msgstr "Mở menu bằng cách di chuột qua chúng"

#. settings-schema.json->open-by-hover->tooltip
msgid "Open all menus when you move the mouse pointer over them"
msgstr "Mở tất cả các menu khi bạn di chuyển con trỏ chuột qua chúng"

#. settings-schema.json->open-by-hover-delay->description
msgid "Hover delay"
msgstr "Độ trễ khi di chuột"

#. settings-schema.json->open-by-hover-delay->units
msgid "milliseconds"
msgstr "miligiây"

#. settings-schema.json->bind-to-workspace->description
msgid "Bind to workspace"
msgstr "Liên kết với không gian làm việc"

#. settings-schema.json->bind-to-workspace->tooltip
msgid "Show the applet only on the selected workspace"
msgstr "Chỉ hiển thị applet trên không gian làm việc đã chọn"

#. settings-schema.json->fixed-menu-width->description
msgid "Fixed Menu Width"
msgstr "Chiều rộng menu cố định"

#. settings-schema.json->fixed-menu-width->tooltip
msgid "Based on the largest submenu item, to prevent menu width flickering"
msgstr ""
"Dựa trên mục menu phụ lớn nhất, để ngăn chặn hiện tượng nhấp nháy theo "
"chiều rộng của menu"

#. settings-schema.json->visible-app-icons->description
msgid "Show Application Icons"
msgstr "Hiển thị biểu tượng ứng dụng"

#. settings-schema.json->visible-app-icons->tooltip
msgid "Show Application Icons in Popup-Menu"
msgstr "Hiển thị biểu tượng ứng dụng trong menu bật lên"

