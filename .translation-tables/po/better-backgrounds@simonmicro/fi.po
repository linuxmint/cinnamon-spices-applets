msgstr "Sovelma"

#. settings-schema.json->applet-store->title
msgid "Save Pictures"
msgstr "Tallenna kuvat"

#. settings-schema.json->effects->title
msgid "Image effects"
msgstr "Kuvatehosteet"

#. settings-schema.json->changeon->title
msgid "Triggers"
msgstr "Käynnistimet"

