#: applet.js:33
msgid "Generate Random Password"
msgstr "Véletlenszerű jelszó generálása"

#: applet.js:42
msgid "Please enable at least one value in settings."
msgstr "Kérjük, engedélyezzen legalább egy értéket a beállításokban."

#: applet.js:49
msgid "Copy"
msgstr "Másolás"

#. metadata.json->description
msgid "Configurable Password Generator"
msgstr "Konfigurálható Jelszógenerátor"

#. metadata.json->name
msgid "Password Generator"
msgstr "Jelszógenerátor"

#. settings-schema.json->password-settings-header->description
msgid "Password Settings"
msgstr "Jelszó beállítások"

#. settings-schema.json->password-length->units
msgid "characters"
msgstr "karakter"

#. settings-schema.json->password-length->description
msgid "Password Length"
msgstr "Jelszó hossza"

#. settings-schema.json->password-length->tooltip
msgid "The password can be any length from 1 to 32 characters."
msgstr "A jelszó bármilyen hosszúságú lehet 1 és 32 karakter között."

#. settings-schema.json->include-lowercase->description
msgid "Include Lowercase Letters"
msgstr "Kisbetűket tartalmazzon"

#. settings-schema.json->include-uppercase->description
msgid "Include Uppercase Letters"
msgstr "Tartalmazzon nagybetűket"

#. settings-schema.json->include-numbers->description
msgid "Include Numbers"
msgstr "Tartalmazzon a számokat"

#. settings-schema.json->include-symbols->description
msgid "Include Symbols"
msgstr "Tartalmazzon a szimbólumokat"

#. settings-schema.json->copy-settings-header->description
msgid "Copy Settings"
msgstr "Másolási beállítások"

#. settings-schema.json->auto-copy->description
msgid "Automatically Copy To Clipboard"
msgstr "Automatikus másolás a vágólapra"

#. settings-schema.json->auto-copy->tooltip
msgid ""
"When clicking the lock icon, instead of opening a window where the password "
"can be copied it is instead automatically copied to your Clipboard."
msgstr ""
"Ha a lakat ikonra kattint, ahelyett, hogy megnyitna egy ablakot, ahol a "
"jelszót le lehet másolni, a rendszer automatikusan a vágólapra másolja."

#. settings-schema.json->play-sound->description
#. settings-schema.json->sound-file->description
msgid "Play sound on copy"
msgstr "Hang lejátszása a másoláskor"

#. settings-schema.json->play-sound->tooltip
msgid ""
"Toggles whether or not a sound is played when the password is copied to the "
"Clipboard"
msgstr ""
"Bekapcsolja, hogy a jelszó vágólapra másolásakor hangot halljon-e vagy sem"

#. settings-schema.json->sound-file->tooltip
msgid "Select a sound to play when the password is copied"
msgstr "Válassza ki a jelszó másolásakor lejátszani kívánt hangot"
