{
    "head": {
        "type": "header",
        "description": "Window Title Settings"
    },
    "title-len": {
        "type": "entry",
        "description": "Maximum title lenght",
        "default": "60"
    },
    "title-bg": {
        "type": "entry",
        "description": "Title focused background color",
        "default": "#5dabf9"
    },
    "title-regex": {
        "type": "entry",
        "description": "Apply regex to title to remove string, before showing",
        "default": "(?:https?|about):.+? - ?"
    }
}
