#. collapsible-systray@feuerfuchs.eu->metadata.json->name
msgid "Collapsible Systray"
msgstr "Pienennä paneeliin"

#. collapsible-systray@feuerfuchs.eu->metadata.json->description
msgid "A replacement for the abandoned System Tray Collapsible Cinnamon applet"
msgstr "Korvaa hylätyn System Tray Cinnamon sovelman"

#. collapsible-systray@feuerfuchs.eu->settings-schema.json->expand-on-
#. hover->description
msgid "Expand on hover"
msgstr "Laajenna osoittamalla"

#. collapsible-systray@feuerfuchs.eu->settings-schema.json->animation-
#. duration->description
msgid "Animation duration"
msgstr "Animaation kesto"

#. collapsible-systray@feuerfuchs.eu->settings-schema.json->animation-
#. duration->units
#. collapsible-systray@feuerfuchs.eu->settings-schema.json->collapse-on-leave-
#. delay->units
#. collapsible-systray@feuerfuchs.eu->settings-schema.json->expand-on-hover-
#. delay->units
msgid "milliseconds"
msgstr "millisekuntia"

#. collapsible-systray@feuerfuchs.eu->settings-schema.json->collapse-on-leave-
#. delay->description
msgid "Collapse on un-hover delay"
msgstr "Pienennä kun viive täyttyy"

#. collapsible-systray@feuerfuchs.eu->settings-schema.json->vertical-collapse-
#. icon-name->description
msgid "Vertical collapse icon"
msgstr "Pienennyksen pystykuvake"

#. collapsible-systray@feuerfuchs.eu->settings-schema.json->vertical-collapse-
#. icon-name->tooltip
#. collapsible-systray@feuerfuchs.eu->settings-schema.json->horizontal-
#. collapse-icon-name->tooltip
#. collapsible-systray@feuerfuchs.eu->settings-schema.json->horizontal-expand-
#. icon-name->tooltip
#. collapsible-systray@feuerfuchs.eu->settings-schema.json->vertical-expand-
#. icon-name->tooltip
msgid "Enter either the icon's qualified name or it's full path"
msgstr "Anna kuvakkeen oikea nimi tai sen koko polku"

#. collapsible-systray@feuerfuchs.eu->settings-schema.json->appearance->title
#. collapsible-systray@feuerfuchs.eu->settings-schema.json->appearance-
#. section->title
msgid "Appearance"
msgstr "Ulkoasu"

#. collapsible-systray@feuerfuchs.eu->settings-schema.json->behavior-
#. section->title
#. collapsible-systray@feuerfuchs.eu->settings-schema.json->behavior->title
msgid "Behavior"
msgstr "Käyttäytyminen"

#. collapsible-systray@feuerfuchs.eu->settings-schema.json->horizontal-
#. collapse-icon-name->description
msgid "Horizontal collapse icon"
msgstr "Pinennyksen vaakakuvake"

#. collapsible-systray@feuerfuchs.eu->settings-schema.json->horizontal-expand-
#. icon-name->description
msgid "Horizontal expand icon"
msgstr "Laajennuksen vaakakuvake"

#. collapsible-systray@feuerfuchs.eu->settings-schema.json->icon-visibility-
#. list->description
msgid "Internal use"
msgstr "Sisäiseen käyttöön"

#. collapsible-systray@feuerfuchs.eu->settings-schema.json->sort-
#. icons->description
msgid "Sort icons by name"
msgstr "Järjestä kuvakkeet nimen mukaan"

#. collapsible-systray@feuerfuchs.eu->settings-schema.json->sort-
#. icons->tooltip
msgid "If unchecked, the order of the icons will be pretty much random"
msgstr "Jos ei ole valittuna, kuvakkeiden järjestys on melko satunnainen"

#. collapsible-systray@feuerfuchs.eu->settings-schema.json->animation-
#. support->description
msgid "Animation support (disable if icons stop updating)"
msgstr "Animaatiotuki (poista käytöstä, jos kuvakkeet ei päivity)"

#. collapsible-systray@feuerfuchs.eu->settings-schema.json->animation-
#. support->tooltip
msgid "Disable if you have issues with icons not updating themselves."
msgstr ""
"Poista käytöstä, jos sinulla on ongelmia kuvakkeiden kanssa, jotka eivät "
"päivity."

#. collapsible-systray@feuerfuchs.eu->settings-schema.json->vertical-expand-
#. icon-name->description
msgid "Vertical expand icon"
msgstr "Laajennuksen pystykuvake"

#. collapsible-systray@feuerfuchs.eu->settings-schema.json->tray-icon-
#. padding->description
msgid "Padding of tray icons"
msgstr "Paneelin kuvakkeiden täyttö"

#. collapsible-systray@feuerfuchs.eu->settings-schema.json->tray-icon-
#. padding->units
msgid "px"
msgstr "px"

#. collapsible-systray@feuerfuchs.eu->settings-schema.json->init-
#. delay->description
msgid "Startup collapse delay"
msgstr "Käynnistyksen pienennysviive"

#. collapsible-systray@feuerfuchs.eu->settings-schema.json->init-
#. delay->tooltip
msgid "The delay until the applet automatically collapses upon startup"
msgstr "Käynnistysviive, jonka jälkeen sovelma pienentyy automaattisesti."

#. collapsible-systray@feuerfuchs.eu->settings-schema.json->init-delay->units
msgid "seconds"
msgstr "sekuntia"

#. collapsible-systray@feuerfuchs.eu->settings-schema.json->expand-on-hover-
#. delay->description
msgid "Expand on hover delay"
msgstr "Laajenna osoittamalla viive"

#. collapsible-systray@feuerfuchs.eu->settings-schema.json->collapse-on-
#. leave->description
msgid "Collapse on un-hover"
msgstr "Pienennä kun et osoita"

#. collapsible-systray@feuerfuchs.eu->settings-schema.json->no-hover-for-tray-
#. icons->description
msgid "Disable hover effect for tray icons"
msgstr "Poista osoittamisen tehoste käytöstä"

#. collapsible-systray@feuerfuchs.eu->settings-schema.json->no-hover-for-tray-
#. icons->tooltip
msgid ""
"If you have problems with the hover effect or it simply doesn't look good, "
"you should enable this setting"
msgstr ""
"Jos sinulla on ongelmia osoittamisen tehosteen kanssa tai se ei näytä "
"hyvältä, ota tämä asetus käyttöön"
