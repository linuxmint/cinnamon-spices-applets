#. settings-schema.json->head0->description
msgid "Settings for wine-utils@elblake"
msgstr "Wine-utils@elblake beállításai"

#. settings-schema.json->head1->description
msgid "General"
msgstr "Általános"

#. settings-schema.json->show-launchers->description
msgid "Show Launchers"
msgstr "Indítók megjelenítése"

#. settings-schema.json->show-launchers->tooltip
msgid "Show some of the Wine launchers if they are installed."
msgstr "Jelenítsen meg néhány telepített Wine indítót."

#. settings-schema.json->show-run-command->description
msgid "Show 'Run Command...'"
msgstr "Jelenítse meg a „Futtatási parancs…” elemet"

#. settings-schema.json->show-run-command->tooltip
msgid "For running a Wine command."
msgstr "Egy Wine parancs futtatásához."

#. settings-schema.json->show-cmd->description
msgid "Show 'Command Prompt'"
msgstr "Jelenítse meg a „Parancssor” elemet"

#. settings-schema.json->show-cmd->tooltip
msgid "For easy access to the Wine command prompt."
msgstr "A Wine parancssor könnyű eléréséhez."

#. settings-schema.json->show-admin-programs-menu->description
msgid "Show the Configuration menu"
msgstr "Jelenítse meg a „Beállítások” menüt"

#. settings-schema.json->show-admin-programs-menu->tooltip
msgid ""
"Show a menu of useful Wine utilities that typically need to be accessed by "
"CLI."
msgstr ""
"A hasznos Wine segédprogramok menüjének megjelenítése, amelyeket általában "
"a parancssorból kellene használnia."

#. settings-schema.json->show-programs-menu->description
msgid "Show the Programs menu"
msgstr "Jelenítse meg a Programok menüt"

#. settings-schema.json->show-programs-menu->tooltip
msgid ""
"Show a menu to access programs directly from Wine's installed program "
"shortcuts."
msgstr ""
"Menü megjelenítése a programok közvetlen eléréséhez a Wine telepített "
"program parancsikonjaiból."

#. settings-schema.json->use-dark-theme-icons->description
msgid "Use dark theme icons when available."
msgstr "Ha elérhető, használjon sötét téma ikonokat."

